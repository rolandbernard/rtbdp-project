<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%highlight(%d{YYYY-dd-MM HH:mm:ss.SSS} \(%level\)) %msg [%thread %logger{15}]%n</pattern>
		</encoder>
	</appender>
	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>
	<logger name="org.apache.kafka" level="WARN" />
	<logger name="com.rolandb" level="DEBUG" />
</configuration>
